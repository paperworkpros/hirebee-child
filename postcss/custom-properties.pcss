
:root {
	--color-primary: #0076cb;
	--color-secondary: #005ea6;
	--color-dark: #373e50;
	--color-darker: #2a3245;
	--color-light: #f9f9f9;
	--color-border: #eaeaea;
	--color-body: #5d5f5e;
	--color-shadow: #efefef;
	--color-info: #e8f2fa;
	--border-radius: 4px;
	--border: 1px solid var(--color-border);
	--transition: all .2s ease;
	--shadow: 0px 2px 1px 0px var(--color-shadow);
}
